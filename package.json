  "scripts": {
    "start": "npm run watch",
    "watch": "rollup -c --environment BUILD:development --watch",
    "build": "rollup -c --environment BUILD:prod",
    "test": "jest",
    "coverage": "jest --coverage",
    "preversion": "npm test",
    "postversion": "git push origin --tags && shx rm -rf dist",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -s ./public -p 6006 --no-dll",
    "build-storybook": "build-storybook -s ./public --no-dll",
    "tailwind": "postcss .storybook/tailwind.scss -o public/styles.css"
  },
