{
  "name": "@cheesebit/ui",
  "version": "0.0.1",
  "description": "CheeseBit React components",
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:prod",
    "build:watch": "rollup -c -w --environment BUILD:dev",
    "postbuild": "npm pack && tar -xvzf *.tgz && rimraf -rf package *.tgz",
    "prepublishOnly": "rimraf -rf build && npm run build",
    "start": "npm-run-all --parallel build:watch",
    "start:api": "json-server --watch ./demo/fixtures/db.json",
    "test": "mocha --require @babel/register --require test/index.js --require test/dom.js --require ignore-styles src/**/*.test.js",
    "preversion": "npm test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "ramda": "^0.26.1",
    "uuid": "^3.4.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-ramda": "^2.0.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chance": "^1.1.4",
    "css-loader": "^3.4.2",
    "css-mediaquery": "^0.1.2",
    "domready": "^1.0.8",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.2.1",
    "json-server": "^0.15.1",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-bundle-html": "^0.2.2",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "2.0.1",
    "sinon": "^7.5.0",
    "style-loader": "^1.1.3"
  },
  "author": "Hotmart",
  "homepage": "https://hotmart-org.github.io/club-custom-components",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hotmart-Org/club-custom-components"
  }
}
